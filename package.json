{
  "name": "nestjs-practice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "migrate": "prisma migrate dev --name init",
    "deploy": "prisma migrate deploy",
    "deploy:test": "dotenv -e .env.test -- npm run deploy",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:test": "dotenv -e .env.test -- npm run start:dev",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "npm run deploy:test",
    "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache --config ./test/e2e/jest.config.ts",
    "pretest:int": "npm run deploy:test",
    "test:int": "dotenv -e .env.test -- jest -i --no-cache --watch --config ./test/integration/jest.config.ts",
    "compose:db": "docker-compose up -d database",
    "compose:up": "docker-compose up -d",
    "compose:test": "docker-compose -f compose.test.yml --env-file ./.env.test up -d",
    "compose:down": "docker-compose down --remove-orphans",
    "compose:restart": "docker-compose restart"
  },
  "dependencies": {
    "@fluidjs/multer-cloudinary": "^1.0.4",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "^11.1.9",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.9",
    "@prisma/client": "^7.0.1",
    "argon2": "^0.44.0",
    "axios": "^1.13.2",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.8.0",
    "helmet": "^8.1.0",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.4.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@types/cheerio": "^1.0.0",
    "@types/express": "^5.0.5",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.17.3",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^2.1.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
